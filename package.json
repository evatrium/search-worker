{
  "name": "search-worker",
  "version": "1.0.0",
  "description": "Simple fast text search that utilizes a web worker",
  "type": "module",
  "source": "src/index.js",
  "exports": {
    "require": "./lib/search-worker.cjs",
    "default": "./lib/search-worker.modern.js"
  },
  "main": "./lib/search-worker.cjs",
  "module": "./lib/search-worker.module.js",
  "unpkg": "./lib/search-worker.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/iosio/search-worker.git"
  },
  "license": "MIT",

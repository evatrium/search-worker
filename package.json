{
  "name": "search-worker",
  "version": "0.0.7",
  "description": "Simple fast text search that utilizes a web worker",
  "repository": {
    "type": "git",
    "url": "https://github.com/iosio/search-worker.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "lib"
  ],
  "keywords": [
    "text search",
    "fuzy search",
    "web worker"
  ],
  "scripts": {
    "start": "rollup -c -w --environment project:demo",
    "build": "rollup -c --environment project:lib",
    "test": "jest --config config/jest.js",
    "test:watch": "jest --config config/jest.js --watch"
  },
  "rollupConfig": {
    "project": {
      "demo": {
        "preset": "dev",
        "input": "demo/src/index.js",
        "html": "demo/src/index.html",
        "output": "demo/build"
      },
      "lib": {
        "preset": "lib",
        "external": [
          "greenlet"
        ]
      }
    }
  },
  "dependencies": {
    "greenlet": "^1.1.0",
    "preact-virtual-list": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-modules": "^0.1.3",
    "@iosio/babel-plugin-jcss": "^0.7.12",
    "@iosio/babel-plugin-minify-literals": "^0.7.12",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-url": "^4.0.2",
    "autoprefixer": "^9.7.4",
    "babel-jest": "^25.1.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-preset-preact": "^1.0.0",
    "preact": "^10.3.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-index-html": "^1.10.2",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-postcss": "^2.4.1",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "serve": "^11.3.0",
    "tiny-glob": "^0.2.6"
  },
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 ChromeAndroid major versions",
    "last 2 Firefox major versions",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions"
  ]
}

{
  "name": "search-worker",
  "version": "0.0.7",
  "description": "Simple fast text search that utilizes a web worker",
  "repository": {
    "type": "git",
    "url": "https://github.com/iosio/search-worker.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "lib"
  ],
  "keywords": [
    "text search",
    "fuzy search",
    "web worker"
  ],
  "scripts": {
    "start": "rollup -c -w --environment project:demo",
    "build": "rollup -c --environment preset:lib",
    "test": "exit 0"
  },
  "rollupConfig": {
    "project": {
      "demo": {
        "preset": "dev",
        "input": "demo/src/index.js",
        "html": "demo/src/index.html",
        "output": "demo/build"
      }
    }
  },
  "dependencies": {
    "greenlet": "^1.1.0"
  },
  "devDependencies": {
    "preact": "^10.3.4",
    "preact-virtual-list": "^0.3.1",
    "rollup-configured": "^0.0.9"
  }
}
